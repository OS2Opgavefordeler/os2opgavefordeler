# jboss-cli.sh --connect --file=test.cli

if (outcome != failure) of /system-property=topicrouter.url.home:add()
	/system-property=topicrouter.url.home:write-attribute(name=value, value="http://localhost:9001")
end-if

if (outcome != failure) of /system-property=topicrouter.url.openid.callback:add()
	/system-property=topicrouter.url.openid.callback:write-attribute(name=value, value="http://localhost:8080/TopicRouter/rest/auth/authenticate")
end-if
